{
  "name": "echopilot-ai",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@monaco-editor/react": "^4.7.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "masonry-layout": "^4.2.2",
    "openai": "^5.13.1",
    "proxy-middleware": "^0.15.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:external": "DANGEROUSLY_DISABLE_HOST_CHECK=true HOST=0.0.0.0 react-scripts start",
    "build:frontend": "react-scripts build",
    "start:backend": "cd python_backend && python app.py",
    "start:server": "node server/index.js",
    "start:websocket": "node websocket-server.js",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:server\" \"npm run start:websocket\" \"npm start\"",
    "start:all-external": "concurrently \"npm run start:backend\" \"npm run start:server\" \"npm run start:websocket\" \"npm run start:external\"",
    "python-server": "python python-backend/start_server.py",
    "api-server": "node server/index.js",
    "websocket-server": "node src/server/websocket-server.js",
    "dev-all": "concurrently \"npm run python-server\" \"npm run api-server\" \"npm run websocket-server\" \"npm start\"",
    "dev": "concurrently \"npm run api-server\" \"npm run websocket-server\" \"npm start\"",
    "dev-ws": "concurrently \"npm run websocket-server\" \"npm start\"",
    "dev:external": "concurrently \"npm run api-server\" \"npm run websocket-server\" \"npm run start:external\"",
    "external": "node start-external.js",
    "dev:setup": "node start-development.js",
    "dev:full": "node start-development.js && concurrently \"npm run api-server\" \"npm run websocket-server\" \"npm start\"",
    "dev:complete": "node start-development.js && concurrently \"npm run python-server\" \"npm run api-server\" \"npm run websocket-server\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "http-proxy-middleware": "^3.0.5"
  }
}
